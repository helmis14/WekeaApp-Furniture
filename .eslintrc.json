{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "semi": "error",
    "quotes": ["error", "double"],
    "indent": ["error", 2]
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "implicit-arrow-linebreak": 0,
    "react/no-unstable-nested-components": 0,
    "object-curly-newline": 0,
    "import/no-unresolved": 1,
    "react/react-in-jsx-scope": 0
  }
}
